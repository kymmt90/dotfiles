[user]
	name = Kohei Yamamoto
	email = kymmt90@gmail.com
[core]
	editor = vi
	excludesfile = ~/.gitignore
[color]
	ui = true
[alias]
	clean-branch = !git branch | grep -v '*' | gxargs --verbose -n 1 git branch -d
[push]
	default = simple
[status]
	showUntrackedFiles = all
[help]
	autocorrect = 1
[ghq]
	root = ~/Repositories
[ghq "https://git.pepabo.com/"]
	vcs = git
