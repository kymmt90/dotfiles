[user]
	name = Kohei Yamamoto
	email = kymmt90@gmail.com
[core]
	editor = vi
	excludesfile = ~/.gitignore
[color]
	ui = true
[alias]
	clean-branch = !git branch | grep -v '^\\* master' | xargs git branch -d
[pager]
        diff = diff-highlight | less
        log = diff-highlight | less
        show = diff-highlight | less
[push]
	default = simple
[status]
	showUntrackedFiles = all
[help]
	autocorrect = 1
[ghq]
	root = ~/Repositories
[ghq "https://git.pepabo.com/"]
	vcs = git
