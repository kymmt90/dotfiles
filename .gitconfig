[user]
	name  = Kohei Yamamoto
	email = kymmt90@gmail.com
[core]
	editor = vi
	excludesfile = ~/.gitignore
[color]
	ui = true
[alias]
	clean-branch = !git branch | grep -v '*' | gxargs --verbose -n 1 git branch -d
[pager]
	diff = diff-highlight | less
	log  = diff-highlight | less
	show = diff-highlight | less
[push]
	default = simple
[help]
	autocorrect = 1
