[user]
	name  = Kohei Yamamoto
	email = kymmt90@gmail.com
[core]
	editor = vi
	excludesfile = ~/.gitignore
[color]
	ui = true
[alias]
	ae   = add -e
	ap   = add -p
	br   = branch
	ci   = commit
	cim  = commit -m
	ciam = commit -am
	co   = checkout
	cb   = checkout -b
	di   = diff
	dc   = diff --cached
	gr   = grep -n
	l    = log
	m    = merge
	ol   = log --oneline
	olg  = log --oneline --graph
	p    = push
	s    = show
	st   = status
[pager]
	diff = diff-highlight | less
	log  = diff-highlight | less
	show = diff-highlight | less
[push]
	default = simple
[help]
	autocorrect = 1
